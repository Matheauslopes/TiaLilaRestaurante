<script>
document.addEventListener("DOMContentLoaded", function() {
    const orderForm = document.getElementById("carrinho");
    const orderButton = document.getElementById("order-button");

    orderButton.addEventListener("click", function() {
        const pratoInputs = document.querySelectorAll('input[name="prato"]:checked');
        const bebidaInputs = document.querySelectorAll('input[name="bebida"]:checked');
        
        let pedido ="Oi Tia Lila, gostaria de pedir uma quentinha de ";
        
        pratoInputs.forEach(function(input) {
            pedido += `${input.value}\n`;
        });
        
        bebidaInputs.forEach(function(input) {
            pedido += `com ${input.value}`;
        });

        let mensagem = ", pra entregar, por favor!"
        
        const whatsappLink =`https://wa.me/+5522998553415?text=${encodeURIComponent(pedido)+(mensagem)}`;
        window.location.href = whatsappLink;
    });
});
</script>
